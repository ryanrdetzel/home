[core]
    editor = vim
	repositoryformatversion = 0
	filemode = true
	bare = false
	logallrefupdates = true

[color]
    diff = auto
    status = auto
    branch = auto

[color "branch"]
    current = yellow reverse
    remote = green bold
    local = blue bold

[color "diff"]
    meta = blue bold
    frag = magenta bold
    old = red bold
    new = green bold
